<?xml version="1.0" encoding="utf-8"?>
<project>
    <meta title="calendar" package="calendar" version="0.0.1" company="Nick Grebenshikov" />

    <!-- output -->
    <app main="Main" file="calendar" path="bin" />

    <window width="0" height="0" background="0xFFFFFF" fps="60" if="html5 || flash"/>
    <window width="800" height="600" background="0xFFFFFF" fps="125" unless="html5 || flash" />

    <!-- classpath, haxe libs -->
    <source path="src" />
    <source name="gen" />

    <set name="html5-backend" value="openfl-snapsvg"/>

    <haxelib name="openfl" />
    <haxelib name="aswing"/>
    <haxelib name="hml"/>
    <haxelib name="bindx"/>

    <template path="templates" />

</project>