<xs:complexType name="FlatRadioButtonUISchemaTypeValue">
<xs:choice>
<xs:element ref="FlatCheckBoxUI" />
<xs:element ref="FlatRadioButtonUI" />
</xs:choice>
</xs:complexType>

<xs:complexType name="FlatTextFieldUISchemaTypeValue">
